## Dashboard API Service environment (copy values into your local .env)
## NOTE: .env is typically gitignored. Do not commit secrets.
##
## Quick local testing without Supabase login:
## - Set AUTH_DISABLED=true
## - Provide a valid DATABASE_URL
AUTH_DISABLED=true

# Supabase Postgres connection string (session pooler, port 5432)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/postgres

# Required unless AUTH_DISABLED=true
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_JWT_SECRET=YOUR_SUPABASE_JWT_SECRET

RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000
PORT=5001
